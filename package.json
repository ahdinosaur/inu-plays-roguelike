{
  "name": "inu-plays-rougelike",
  "version": "0.0.0",
  "private": true,
  "description": "'Twitch Plays Pok√©mon'-style rouglelike game using inu",
  "main": "app/server",
  "browser": "app/client",
  "scripts": {
    "test": "tape test/**/*.js",
    "client": "budo app/client:bundle.js -d app --live -- -d",
    "server": "node-dev app/server",
    "start": "npm-run-all -p client server",
    "bundle": "browserify app -g envify -g uglifyify",
    "build": "npm run --silent bundle -- -o app/bundle.js",
    "disc": "npm run --silent bundle -- --full-paths | discify --open",
    "push": "gh-pages -d app",
    "deploy": "npm-run-all -s build push",
    "lint": "standard",
    "format": "standard-format"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/inu-plays-rougelike.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/inu-plays-rougelike/issues"
  },
  "homepage": "https://github.com/ahdinosaur/inu-plays-rougelike#readme",
  "devDependencies": {
    "babel-register": "^6.9.0",
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^1.6.0",
    "standard": "^7.1.2",
    "standard-format": "^2.2.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "inu": "github:ahdinosaur/inu#run-all-streams",
    "lodash": "^4.13.1",
    "pull-cat": "^1.1.9",
    "pull-json-doubleline": "ahdinosaur/pull-json-doubleline#v2",
    "pull-keys": "^1.0.0",
    "pull-many": "^1.0.6",
    "pull-pushable": "^2.0.0",
    "pull-ws-server": "^1.9.2",
    "tcomb": "^3.2.2"
  }
}
